{
  "cli": {
    "version": ">= 16.19.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "testflight": {
      "distribution": "store",        
      "autoIncrement": true,
      "channel": "preview"           
    },
    "production": {
      "distribution": "store",       
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "testflight": {},
    "production": {}
  }
}
