{
  "expo": {
    "name": "DCOasis",
    "slug": "dcoasis",
    "version": "1.0.0",
    "owner": "daniel.chen0113",
    "orientation": "portrait",
    "icon": "./assets/images/DCOasis_Icon.png",
    "scheme": "dcoasis",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "backgroundColor": "#0B1220",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "bundleIdentifier": "com.daniel.chen0113.dcoasis",
      "buildNumber": "7",
      "infoPlist": {
        "NSCameraUsageDescription": "DCOasis needs camera access so workers can take a photo when clocking in.",
        "NSPhotoLibraryUsageDescription": "Allow DCOasis to select photos for clock-ins.",
        "NSPhotoLibraryAddUsageDescription": "Allow DCOasis to save clock-in photos to your library.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.daniel.chen0113.dcoasis",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES"
      ]
    },
    "androidStatusBar": {
      "backgroundColor": "#0B1220",
      "barStyle": "light-content",
      "translucent": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-camera",
      "expo-image-picker",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#0B1220"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": { "typedRoutes": true },
    "extra": {
      "router": {},
      "eas": { "projectId": "3f52eecb-c1d1-4c05-89fc-ff257e76152f" }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/3f52eecb-c1d1-4c05-89fc-ff257e76152f"
    }
  }
}
